{
  "name": "convert-string-time",
  "description": "Simple utility to convert between 12/24 hours string formats",
  "version": "4.0.1",
  "license": "MIT",
  "author": "Ahmed El Gabri <ahmed@gabri.me> (https://gabri.me)",
  "main": "dist/index.js",
  "module": "dist/convert-string-time.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "time",
    "format",
    "convert",
    "24 hours",
    "12 hours"
  ],
  "scripts": {
    "start": "tsdx watch ./src",
    "build": "tsdx build ./src",
    "test": "tsdx test",
    "lint": "prettier --list-different && tsdx lint ./src",
    "publish": "np",
    "prepare": "tsdx build ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "proseWrap": "always",
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "husky": "^4.2.3",
    "np": "^7.5.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
